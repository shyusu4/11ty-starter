{
  "name": "11ty-starter",
  "version": "1.0.0",
  "description": "11ty starter project",
  "main": "index.js",
  "scripts": {
    "sass": "npx sass --style=compressed src/assets/sass/style.scss src/assets/css/style.css",
    "watch:sass": "npm run sass -- --watch",
    "watch:eleventy": "npx @11ty/eleventy --serve",
    "start": "npm-run-all sass --parallel watch:*",
    "build": "npx @11ty/eleventy",
    "checks": "html-validate . && eslint .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "serve": "npx @11ty/eleventy --serve",
    "validate": "html-validate ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shyusu4/11ty-starter.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shyusu4/11ty-starter/issues"
  },
  "homepage": "https://github.com/shyusu4/11ty-starter#readme",
  "devDependencies": {
    "@11ty/eleventy": "^3.1.1",
    "@11ty/eleventy-img": "^6.0.4",
    "@11ty/eleventy-navigation": "^1.0.4",
    "@11ty/eleventy-plugin-rss": "^2.0.4",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.1",
    "stylelint-config-standard": "^38.0.0",
    "stylelint": "^16.21.0",
    "eslint": "^9.29.0",
    "globals": "^16.2.0",
    "html-validate": "^9.7.0",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "sass": "^1.89.2"
  }
}
